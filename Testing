local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("DW/UMG Hub [ALPHATEST]", "GrapeTheme")


--Main
local Main = Window:NewTab("DWMain")
local MainSection = Main:NewSection("DW")


MainSection:NewButton("Godwill", "Simple script, Wanna team kill?", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/Aidez/decaying_winter/main/GOODWILL_COMMAND_SCRIPT'),true))()
end)


MainSection:NewButton("Became SledgeQueen!", "Simp? I don't know you are or not, But I don't...", function()
    loadstring(game:HttpGet(('https://pastebin.com/raw/s9eURmiU')))()
end)


